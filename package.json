{
  "name": "snowbot-ui",
  "version": "0.0.1",
  "description": "UI for snowbot",
  "main": "./dist",
  "scripts": {
    "test": "npm run test:unit -- --coverage",
    "test:watch": "npm run test:unit -- --watchAll",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "old-build": "cross-env NODE_ENV=production webpack",
    "old-start": "cross-env NODE_ENV=development webpack-dev-server",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "test:unit": "cross-env BABEL_ENV=test jest",
    "test:rebuildSnapshot": "cross-env BABEL_ENV=test jest --updateSnapshot"
  },
  "dependencies": {
    "@jsluna/accordion": "^1.0.6",
    "@jsluna/button": "^1.0.7",
    "@jsluna/card": "^1.0.6",
    "@jsluna/fonts": "^1.0.6",
    "@jsluna/footer": "^1.0.6",
    "@jsluna/form": "^1.1.3",
    "@jsluna/foundation": "^2.3.0",
    "@jsluna/grid": "^1.0.6",
    "@jsluna/header": "^2.0.8",
    "@jsluna/icons": "^2.0.6",
    "@jsluna/images": "^3.1.0",
    "@jsluna/link": "^1.1.6",
    "@jsluna/list": "^2.0.2",
    "@jsluna/primer": "^1.0.6",
    "@jsluna/section": "^1.0.6",
    "@jsluna/site-layout": "^2.0.7",
    "@jsluna/tokens": "^1.2.0",
    "@jsluna/typography": "^1.0.6",
    "@jsluna/utils": "^1.0.6",
    "axios": "^0.21.1",
    "classnames": "^2.2.5",
    "connected-react-router": "^6.5.2",
    "cross-env": "^6.0.0",
    "history": "^4.10.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hot-loader": "^4.12.14",
    "react-loadable": "^5.4.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "^1.1.1",
    "svgxuse": "^1.2.6"
  },
  "devDependencies": {
    "node-sass": "^4.12.0"
  },
  "devDepDisabled": {
    "enzyme": "^3.10.0",
    "husky": "^3.0.7",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.1",
    "redux-devtools": "^3.5.0",
    "stylelint": "^9.3.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Kiwi Team",
  "private": true,
  "license": "UNLICENSED"
}
